{
  "name": "paychan-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build:local": "export DOTENV_CONFIG_PATH=config/.env.local && next build && next export",
    "build:dev": "export DOTENV_CONFIG_PATH=config/.env.dev && next build && next export",
    "build:prd": "export DOTENV_CONFIG_PATH=config/.env.prd && next build && next export",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@hookform/resolvers": "^2.9.3",
    "@line/bot-sdk": "^7.5.2",
    "@line/liff": "^2.21.2",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.5",
    "@mui/system": "^5.10.5",
    "@mui/x-data-grid": "^5.17.2",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "firebase": "^9.8.4",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "7.33.1",
    "react-query": "^3.39.1",
    "react-select": "^5.4.0",
    "recoil": "^0.7.4",
    "styled-components": "^5.3.5",
    "use-postal-jp": "^2.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "17.0.40",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.6.2",
    "typescript": "4.7.3"
  }
}
